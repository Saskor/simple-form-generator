{
  "name": "simple-form-generator",
  "version": "1.0.0",
  "description": "simple-form-generator",
  "private": true,
  "scripts": {
    "test": "cross-env jest",
    "test:watch": "cross-env jest --watch",
    "eslint": "cross-env eslint 'src/**/*.{js,ts,tsx}' --cache --cache-location './.cache/eslint/' --fix",
    "stylelint": "cross-env stylelint 'src/**/*.{css,scss}' --cache --cache-location './.cache/stylelint/'",
    "lint": "npm run eslint && npm run stylelint",
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "npm run test && cross-env NODE_ENV=production webpack --mode production --config webpack.config.js",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch --config webpack.config.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open --config webpack.config.js",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "main": "src/index.tsx",
  "keywords": [
    "simple-form-generator"
  ],
  "author": "Saskor <korinskiy1985@gmail.com>",
  "license": "ISC",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup/index.js"
    ]
  },
  "browserslist": [
    "edge >= 16",
    "firefox >= 60",
    "chrome >= 61",
    "safari >= 11",
    "opera >= 48"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@types/enzyme": "^3.10.6",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "canvas": "^2.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "css-modules-typescript-loader": "^4.0.0",
    "csv-loader": "^3.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "fibers": "^4.0.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "4.3.0",
    "jest": "^26.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "2.1.2",
    "sass": "^1.26.11",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "terser-webpack-plugin": "^4.0.0",
    "ts-jest": "^26.4.0",
    "ts-mockito": "^2.6.1",
    "typescript": "^3.9.5",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "@types/lodash": "^4.14.159",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "formik": "^2.2.9",
    "lodash": "^4.17.19",
    "mobx": "^6.1.8",
    "mobx-react": "^7.1.0",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-select": "^5.2.2"
  }
}
